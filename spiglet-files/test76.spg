MAIN
	MOVE TEMP 2 HALLOCATE 8
	MOVE TEMP 3 HALLOCATE 12
	MOVE TEMP 8 Test_start
	HSTORE TEMP 2 0 TEMP 8
	MOVE TEMP 9 Test_next
	HSTORE TEMP 2 4 TEMP 9
	MOVE TEMP 4 4
L0
	MOVE TEMP 10 LT TEMP 4 12
	CJUMP TEMP 10 L1
	MOVE TEMP 11 PLUS TEMP 3 TEMP 4
	MOVE TEMP 12 0
	HSTORE TEMP 11 0 TEMP 12
	MOVE TEMP 4 PLUS TEMP 4 4
	JUMP L0
L1
	HSTORE TEMP 3 0 TEMP 2
	HLOAD TEMP 6 TEMP 3 0
	HLOAD TEMP 7 TEMP 6 0
	MOVE TEMP 13 CALL TEMP 7 ( TEMP 3 )
	MOVE TEMP 5 TEMP 13
	PRINT TEMP 5
END

Test_start [ 1 ]
 BEGIN
	HLOAD TEMP 1 TEMP 0 4
	HLOAD TEMP 3 TEMP 1 0
	HLOAD TEMP 4 TEMP 3 4
	MOVE TEMP 15 CALL TEMP 4 ( TEMP 1 )
	MOVE TEMP 2 TEMP 15
	HSTORE TEMP 0 8 TEMP 2
 RETURN 0
END

Test_next [ 1 ]
 BEGIN
	HLOAD TEMP 1 TEMP 0 4
	HLOAD TEMP 3 TEMP 1 0
	HLOAD TEMP 4 TEMP 3 0
	MOVE TEMP 17 CALL TEMP 4 ( TEMP 1 )
	MOVE TEMP 2 TEMP 17
	HSTORE TEMP 0 8 TEMP 2
	HLOAD TEMP 5 TEMP 0 8
 RETURN TEMP 5
END
